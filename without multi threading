
import time
def double(number):
    for i in number:
        time.sleep(1)
        print('duble',2*i)
def square(number):
    for i in number:
        time.sleep(1)
        print('sqr',i*i)
number=[1,2,3,4,5,6,7]
begin=time.time()

print(double(number))
print(square(number))
end=time.time()
print("time",end-begin)
